name: obs_websocket
version: 2.2.0
description: obs_websocket allows dart-based connections to the Open Broadcaster plugin obs-websocket
repository: https://github.com/faithoflifedev/obsWebsocket
homepage: https://github.com/faithoflifedev/obsWebsocket

environment:
  sdk: '>=2.12.0 <3.0.0'

dependencies:
  args: ^2.1.1
  crypto: ^3.0.1
  json_annotation: ^4.0.1
  universal_io: ^2.0.4
  web_socket_channel: ^2.1.0
  yaml: ^3.1.0

dev_dependencies:
  build_runner: ^2.0.3
  grinder: ^0.9.0
  json_serializable: ^4.1.3
  mustache_template: ^2.0.0
  pedantic: ^1.11.0
  process_run: ^0.12.1
  test: ^1.17.5

dependency_overrides:
  web_socket_channel:
    git: https://github.com/faithoflifedev/web_socket_channel.git